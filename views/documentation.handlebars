<script src='script/newdocumentation.js'></script>
<title>Documentation - LenoxBot</title>
<header>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a href="https://lenoxbot.com" class="navbar-brand text">LenoxBot</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
            aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto" id="mainnavbar">
                <li class="nav-item">
                    <a href="home" class="h5 nav-link">Home</a>
                </li>
                <li class="nav-item active">
                    <a href="documentation" class=" h5 nav-link">Documentation</a>
                </li>
                <li class="nav-item">
                    <a href="https://medium.com/lenoxbot" target="blanc" class=" h5 nav-link">Blog

                    </a>
                </li>
                <li class="nav-item">
                    <a href="https://lenoxbot.com/invite" target="blanc" class=" h5 nav-link">Invite LenoxBot</a>
                </li>
                <li class="nav-item">
                    <a href="https://lenoxbot.com/discord" target="blanc" class=" h5 nav-link">Discord Server</a>
                </li>
                <li class="nav-item">
                    {{#if user}}
                    <a href="servers" class=" h5 nav-link">Dashboard
                        <span class="badge badge-danger">BETA</span>
                    </a>
                    {{/if}}
                </li>
            </ul>
            <ul class="navbar-nav navbar-right" id="mainnavbar">
                <li class="nav-item">
                    {{#if user}}
                    <a href="logout" class=" h5 nav-link ">Log out</a>
                    {{ else }}
                    <a href="loginpressedbutton" class=" h5 nav-link ">Log in</a>
                    {{/if}}
                </li>
                <li class="avatarnavigation">
                    {{#if user}}
                    <a href="https://lenoxbot.com/servers">
                        <img class="img-fluid rounded profilepic-navbar nav-link img-thumbnail" src="https://cdn.discordapp.com/avatars/{{user.id}}/{{user.avatar}}.jpg"
                            height="30" width="50">
                    </a>
                    {{/if}}
                </li>
            </ul>
        </div>
    </nav>
    </div>
</header>
<main>
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <a href="/commands">
                    <button type="button" class="btn btn-primary btn-lg btn-block" style="margin-top: 50px;">Commandlist</button>
                </a>
                <p class="text h1 text-white" style="margin-top: 50px; margin-bottom: 20px;">General FAQ</p>
                <div id="accordion-generalfaq" style="margin-bottom: 50px;">
                    {{#each generalfaq}}
                    <div class="card">
                        <div class="card-header" id="headinggeneralfaq{{number}}">
                            <h5 class="mb-0 text h1">
                                <button class="btn btn-link text collapsed" data-toggle="collapse" data-target="#collapsegeneralfaq{{number}}" aria-expanded="true"
                                    aria-controls="collapsegeneralfaq{{number}}">
                                    {{title}}
                                </button>
                                <span class="documentation-entry" style="float:right;"></span>
                            </h5>
                        </div>

                        <div id="collapsegeneralfaq{{number}}" class="collapse" aria-labelledby="headinggeneralfaq{{number}}" data-parent="#accordion-generalfaq">
                            <div class="card-body" style="margin: 20px;">
                                {{{content}}}
                                <div class="text h6" style="color:rgb(94, 154, 233)">
                                    <i class="far fa-clock"></i> Last updated: {{newdate}} <br><i class="fas fa-user"></i> {{author}}
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
                <p class="text h1 text-white" style="margin-top: 50px; margin-bottom: 20px;">Tutorials</p>
                <div id="accordion-tutorials" style="margin-bottom: 50px;">
                    {{#each tutorials}}
                    <div class="card">
                        <div class="card-header" id="headingtutorials{{number}}">
                            <h5 class="mb-0 text h1">
                                <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapsetutorials{{number}}" aria-expanded="true"
                                    aria-controls="collapsetutorials{{number}}">
                                    {{title}}
                                </button>
                                <span class="documentation-entry" style="float:right;"></span>
                            </h5>
                        </div>

                        <div id="collapsetutorials{{number}}" class="collapse" aria-labelledby="headingtutorials{{number}}" data-parent="#accordion-tutorials">
                            <div class="card-body" style="margin: 20px;">
                                {{{content}}}
                                <div class="text h6" style="color:rgb(94, 154, 233)">
                                    <i class="far fa-clock"></i> Last updated: {{newdate}} <br><i class="fas fa-user"></i> {{author}}
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</main>
<script>
    anchors.options = {
  visible: 'always',
  icon: '#'
};
      anchors.add('.documentation-entry');
</script>