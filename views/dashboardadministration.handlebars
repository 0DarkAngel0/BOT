                            <form class="form-inline" action="/dashboard/{{guilds.id}}/administration/submitprefix"
                                method="POST">
                                <div class="form-group mb-2 col-sm-3">
                                    <label for="newprefix">Prefix:</label>
                                </div>
                                <div class="form-group mx-3 mb-2 col-sm-4 col-lg-3">
                                    <input type="text" class="form-control lb-input-prefix" id="newprefix" name="newprefix"
                                        pattern=".{1,}" required value="{{guilds.prefix}}">
                                </div>
                                <button type="submit" class="btn lb-btn-green mb-2 ml-3">Save</button>
                            </form>
                            <form class="form-inline" action="/dashboard/{{guilds.id}}/administration/submitlanguage"
                                method="POST">
                                <div class="form-group mb-2 col-sm-3">
                                    <label for="newlanguage">Language:</label>
                                </div>
                                <div class="form-group mx-sm-3 mb-2 col-sm-4 col-lg-3">
                                    <select required class="selectpicker" name="newlanguage">
                                        {{#each languages}} {{#if set}}
                                        <option value="{{alias}}" selected>{{name}}</option>
                                        {{else}}
                                        <option value="{{alias}}">{{name}}</option>
                                        {{/if}} {{/each}}
                                    </select>
                                </div>
                                <button type="submit" class="btn lb-btn-green mb-2 ml-3">Save</button>
                            </form>
