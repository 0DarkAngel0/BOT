                                        <option value="{{id}}">#{{name}}</option>
                                        {{/if}} {{/each}}
                                    </select>
                                </div>
                                <button type="submit" class="btn lb-btn-green mb-2 ml-3">Save</button>
                            </form>
                            <form class="form-horizontal" action="/dashboard/{{guilds.id}}/administration/submitwelcomemsg"
                                method="POST">
                                <div class="form-group">
                                    <label class="control-label col-sm" for="newwelcomemsg">Set a new welcome message:</label>
                                    <div class="col-sm-9">
                                        <textarea class="form-control" name="newwelcomemsg" rows="3">{{guilds.welcomemsg}}</textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-offset-3 col-sm-10">
                                        <button type="submit" class="btn lb-btn-green mb-1">Save</button>
                                        <button id="popover-for-msgoptions1" type="button" class="btn lb-btn-grey mb-1"
                                            data-toggle="popover" title="All message options:" data-content="$usertag$<br>$usermention$<br>$username$<br>$userid$<br>$guildname$<br>$guildid$">Show
                                            all message options</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="card mt-3 shadow-sm">
                        <div class="card-header">
                            <p class="h5">Bye Settings</p>
                        </div>
                        <div class="card-body">
                            <form class="form-inline" action="/dashboard/{{guilds.id}}/administration/submittogglebye"
                                method="POST">
                                <div class="form-group mb-2 col-sm-3">
                                    <label for="newbye">Bye:</label>
                                </div>
                                <div class="form-group mx-sm-3 mb-2 col-sm-4 col-lg-3">
                                    <select required class="selectpicker" name="newbye" data-live-search="true"
                                        data-size="7">
                                        {{#each channels}} {{#if byeset}}
                                        <option value="{{id}}" selected>#{{name}}</option>
                                        {{else}}
                                        <option value="{{id}}">#{{name}}</option>
                                        {{/if}} {{/each}}
                                    </select>
                                </div>
                                <button type="submit" class="btn lb-btn-green mb-2 ml-3">Save</button>
                            </form>
                            <form class="form-horizontal" action="/dashboard/{{guilds.id}}/administration/submitbyemsg"
                                method="POST">
                                <div class="form-group">
                                    <label class="control-label col-sm" for="newbyemsg">Set a new bye message:</label>
                                    <div class="col-sm-9">
                                        <textarea class="form-control" name="newbyemsg" rows="3">{{guilds.byemsg}}</textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-offset-3 col-sm-10">
                                        <button type="submit" class="btn lb-btn-green mb-1">Save</button>
                                        <button id="popover-for-msgoptions2" type="button" class="btn lb-btn-grey mb-1"
                                            data-toggle="popover" title="All message options:" data-content="$usertag$<br>$username$<br>$userid$<br>$guildname$<br>$guildid$">Show
                                            all message options</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="card mt-3 shadow-sm">
                        <div class="card-header">
                            <p class="h5">Chatfilter</p>
                        </div>
                        <div class="card-body">
                            <form class="form-inline" action="/dashboard/{{guilds.id}}/administration/submittogglechatfilter"
                                method="POST">
                                <div class="form-group mb-2 col-sm-3">
                                    <label for="newchatfilter">Chatfilter:</label>
                                </div>
                                <div class="form-group mx-sm-3 mb-2 col-sm-4 col-lg-3">
                                    <select required class="selectpicker" name="newchatfilter">
                                        {{#if chatfilterset}}
                                        <option value="true" selected>Activated</option>
                                        <option value="false">Deactivated</option>
                                        {{else}}
                                        <option value="true">Activated</option>
                                        <option value="false" selected>Deactivated</option>
                                        {{/if}}
                                    </select>
                                </div>
                                <button type="submit" class="btn lb-btn-green mb-2 ml-3">Save</button>
                            </form>
                            <form class="form-horizontal" action="/dashboard/{{guilds.id}}/administration/submitchatfilterarray"
                                method="POST">
                                <div class="form-group">
                                    <label class="control-label col-sm" for="newbyemsg">Update chatfilter entries (Each
                                        entry must be separated by a comma):</label>
                                    <div class="col-sm-9">
                                        <textarea class="form-control" name="newchatfilterarray" rows="20" placeholder="word1,word2,word3"
                                            pattern="^\S+$">{{chatfilterarray}}</textarea>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-offset-3 col-sm-10">
                                        <button type="submit" class="btn lb-btn-green mb-2 ml-3">Save</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="card mt-3 shadow-sm">
                        <div class="card-header">
                            <p class="h5">Commands</p>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table">
                                    <tbody>
                                        {{#each commands}}
                                        <tr>
                                            <td>
                                                <div class="col-sm-8">
                                                    <h6>{{help.name}}</h6>
                                                    <p class="text-muted">{{help.description}}</p>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="modal fade" id="{{help.name}}" tabindex="-1" role="dialog"
                                                    aria-labelledby="{{help.name}}Title" aria-hidden="true">
                                                    <div class="modal-dialog modal-dialog-centered" role="document">
                                                        <div class="modal-content">
                                                            <div class="modal-header">
                                                                <h5 class="modal-title" id="exampleModalLongTitle">Settings
                                                                    of {{help.name}} command</h5>
                                                                <button type="button" class="close" data-dismiss="modal"
                                                                    aria-label="Close">
                                                                    <span aria-hidden="true">&times;</span>
                                                                </button>
                                                            </div>
                                                            <form class="form-inline" action="/dashboard/{{../guilds.id}}/administration/{{help.name}}/submitcommandchange"
                                                                method="POST">
                                                                <div class="modal-body">
                                                                    <div class="form-group mb-2">
                                                                        <label for="newblacklistedchannels">Blacklisted
                                                                            channels:</label>
                                                                    </div>
                                                                    <div class="form-group mx-sm-3 mb-2 col-sm-6">
                                                                        <select class="selectpicker" name="newblacklistedchannels"
                                                                            data-live-search="true" multiple title="Choose channel(s)"
                                                                            data-actions-box="true" data-size="7">
                                                                            {{#each ../channels}} {{#inArray
                                                                            ../bannedchannels id}}
                                                                            <option value="{{id}}" selected>#{{name}}</option>
                                                                            {{else}}
                                                                            <option value="{{id}}">#{{name}}</option>
                                                                            {{/inArray}} {{/each}}
                                                                        </select>
                                                                    </div>
                                                                    <hr>
                                                                    <div class="form-group mb-2">
                                                                        <label for="newwhitelistedroles">Whitelisted
                                                                            roles:</label>
                                                                    </div>
                                                                    <div class="form-group mx-sm-3 mb-2 col-sm-6">
                                                                        <select class="selectpicker" name="newwhitelistedroles"
                                                                            data-live-search="true" multiple title="Choose roles(s)"
                                                                            data-actions-box="true" data-size="7">
                                                                            {{#each ../roles}} {{#inArray
                                                                            ../whitelistedroles id}}
                                                                            <option value="{{id}}" selected>@{{name}}</option>
                                                                            {{else}}
                                                                            <option value="{{id}}">@{{name}}</option>
                                                                            {{/inArray}} {{/each}}
                                                                        </select>
                                                                    </div>
                                                                    <hr>
                                                                    <div class="form-group mb-2">
                                                                        <label for="newblacklistedroles">Blacklisted
                                                                            roles:</label>
                                                                    </div>
                                                                    <div class="form-group mx-sm-3 mb-2 col-sm-6">
                                                                        <select class="selectpicker" name="newblacklistedroles"
                                                                            data-live-search="true" multiple title="Choose roles(s)"
                                                                            data-actions-box="true" data-size="7">
                                                                            {{#each ../roles}} {{#inArray
                                                                            ../bannedroles id}}
                                                                            <option value="{{id}}" selected>@{{name}}</option>
                                                                            {{else}}
                                                                            <option value="{{id}}">@{{name}}</option>
                                                                            {{/inArray}} {{/each}}
                                                                        </select>
                                                                    </div>
                                                                    <hr>
                                                                    <div class="form-group mb-2">
                                                                        <label for="newcooldown">Cooldown (in
                                                                            seconds):</label>
                                                                    </div>
                                                                    <div class="form-group mx-sm-3 mb-2 col-sm-6">
                                                                        <input type="number" min="3" max="600" class="form-control"
                                                                            name="newcooldown" required value="{{cooldown}}">
                                                                    </div>
                                                                    <hr>
                                                                    <button type="submit" class="btn lb-btn-green mt-1">Save
                                                                        changes</button>
                                                                </div>
                                                            </form>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="btn-group float-right" role="group">
                                                    <button type="button" class="btn lb-btn-blue" data-toggle="modal"
                                                        data-target="#{{help.name}}">
                                                        Edit
                                                    </button>
                                                    {{#if conf.enabled}}
                                                    <form id="{{help.name}}form">
                                                        <input type="hidden" value="false" name="statuschange" id="{{help.name}}status">
                                                    </form>
                                                    <button type="submit" class="btn lb-btn-red" id="{{help.name}}b">
                                                        Disable
                                                    </button>
                                                    {{else}}
                                                    <form id="{{help.name}}form">
                                                        <input type="hidden" value="true" name="statuschange" id="{{help.name}}status">
                                                    </form>
                                                    <button type="submit" class="btn lb-btn-green" id="{{help.name}}b">
                                                        Enable
                                                    </button>
                                                    {{/if}}
                                                </div>
                                                <script>
                                                    var guildid = '{{../guilds.id}}';
                                                </script>
                                                {{> ajaxscript}}
                                            </td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        $(function () {
            $('#popover-for-msgoptions1').popover({
                container: 'body',
                html: true
            })
            $('#popover-for-msgoptions2').popover({
                container: 'body',
                html: true
            })
        })
    </script>
</main>