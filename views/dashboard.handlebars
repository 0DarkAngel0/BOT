<link rel="stylesheet" type="text/css" href="../../styles/style.css" />
<link rel="stylesheet" type="text/css" href="../../assets/fonts/font-awesome/css/font-awesome.min.css" />
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="../../assets/numscroller/numscroller-1.0.js"></script>
<title>Dashboard - LenoxBot</title>

<header>
	<nav class="navbar navbar-expand-lg navbar-light bg-light">
		<a href="https://lenoxbot.com" class="navbar-brand text">LenoxBot</a>
		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
		    aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>
		<div class="collapse navbar-collapse" id="navbarSupportedContent">
			<ul class="navbar-nav mr-auto" id="mainnavbar">
				<li class="nav-item">
					<a href="../../home" class="text-size-normal nav-link">Home</a>
				</li>
				<li class="nav-item">
					<a href="../../documentation" class=" text-size-normal nav-link">Documentation</a>
				</li>
				<li class="nav-item">
					<a href="https://medium.com/lenoxbot" target="blanc" class=" text-size-normal nav-link">Blog
						<span class="label label-info">NEW</span>
					</a>
				</li>
				<li class="nav-item">
					<a href="https://lenoxbot.com/invite" target="blanc" class=" text-size-normal nav-link">Invite LenoxBot</a>
				</li>
				<li class="nav-item">
					<a href="https://lenoxbot.com/discord" target="blanc" class=" text-size-normal nav-link">Discord Server</a>
				</li>
				<li class="nav-item active">
					{{#if user}}
					<a href="../../servers" class=" text-size-normal nav-link">Dashboard
						<span class="label label-danger">ALPHA</span>
					</a>
					{{/if}}
				</li>
				<li class="nav-item">
					{{#if user}}
					<a href="../../logout" class=" text-size-normal nav-link ">Log out</a>
					{{ else }}
					<a href="../../login" class=" text-size-normal nav-link ">Log in</a>
					{{/if}}
				</li>
			</ul>
		</div>
	</nav>
	</div>
</header>

<main>
	<div class="container">
		<div class="row">
			<div class="col-sm-3">
				<nav class="navbar navbar-dark bg-dark" id="dashboardnav">
					<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#dashboardnavigationbar" aria-controls="dashboardnavigationbar"
					    aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="dashboardnavigationbar">
					<ul class="navbar-nav mr-auto" id="mainnavbar">
						<li class="nav-item nav-link">
							<a href="/dashboard/{{guilds.id}}/overview" class=" text-size-normal nav-link active" role="button">Overview</a>
						</li>
						<li class="nav-item nav-link">
							<a href="/dashboard/{{guilds.id}}/generalsettings" class=" text-size-normal nav-link" role="button">General Settings</a>
						</li>
						<li class="nav-item nav-link">
							<a href="/dashboard/{{guilds.id}}/logs" class=" text-size-normal nav-link" role="button">Logssettings</a>
						</li>
						<li class="nav-item nav-link">
							<a href="/dashboard/{{guilds.id}}/modules" class=" text-size-normal nav-link" role="button">Modules</a>
						</li>
						<li class="nav-item nav-link">
							<a href="/dashboard/{{guilds.id}}/chatfilter" class=" text-size-normal nav-link" role="button">Chatfilter</a>
						</li>
						<li class="nav-item nav-link">
							<a href="/dashboard/{{guilds.id}}/music" class=" text-size-normal nav-link" role="button">Music</a>
						</li>
						<li class="nav-item nav-link">
							<a href="/dashboard/{{guilds.id}}/application" class=" text-size-normal nav-link" role="button">Application</a>
						</li>
						<li class="nav-item nav-link">
							<a href="#" class=" text-size-normal nav-link" role="button">Tickets
								<span class="label label-danger">INCOMING</span>
							</a>
						</li>
					</ul>
					</div>
				</nav>
			</div>
			<div class="col-sm-9">
				<div class="panel-group panel-group-dashboard">
					<div class="panel panel-primary panel-dashboard">
						<div class="panel-heading">
							<p class="text text-white text-size-big">Welcome {{user.username}},</p>
						</div>
					</div>
					<div class="card-group" style="text-align: center;">
						<div class="card text-white bg-primary mb-3 overviewcard" style="width: 16rem;">
							<div class="card-body">
								<span class="numscroller card-text text text-size-big" style="text-align: center;" data-min='1' data-max="{{guilds.memberscount}}"
								    data-delay='0.8' data-increment='1'>{{guilds.memberscount}}</span>
								<p class="text card-title" style="text-align: center;">Members</p>
							</div>
						</div>
						<div class="card text-white bg-primary mb-3 overviewcard" style="width: 16rem;">
							<div class="card-body">
								<span class="numscroller card-text text text-size-big" style="text-align: center;" data-min='1' data-max="{{guilds.channelscount}}"
								    data-delay='0.8' data-increment='1'>{{guilds.channelscount}}</span>
								<p class="text card-title" style="text-align: center;">Channels</p>
							</div>
						</div>
						<div class="card text-white bg-primary mb-3 overviewcard" style="width: 16rem;">
							<div class="card-body">
								<span class="numscroller card-text text text-size-big" style="text-align: center;" data-min='1' data-max="{{guilds.rolescount}}"
								    data-delay='0.8' data-increment='1'>{{guilds.rolescount}}</span>
								<p class="text card-title" style="text-align: center;">Roles</p>
							</div>
						</div>
						<div class="card text-white bg-primary mb-3 overviewcard" style="width: 16rem;">
							<div class="card-body">
								<p class="card-text text text-size-big" style="text-align: center;">{{guilds.prefix}}</p>
								<p class="text card-title" style="text-align: center;">Prefix</p>
							</div>
						</div>
					</div>
					<div class="panel panel-primary panel-dashboard">
						<div class="panel-heading">
							<p class="text text-white text-size-normal">Last logs:</p>
						</div>
						<div class="panel-body">
							<table class="table table-bordered">
								<!-- Header of logs -->
								<tr>
									<th class="text text-size-normal text-black">Action</th>
									<th class="text text-size-normal text-black">Executor</th>
									<th class="text text-size-normal text-black">Date</th>
								</tr>
								<!-- Header of logs -->
								{{#if logs}} {{#each logs}}
								<tr>
									<td class="text text-size-small text-black">{{action}}</td>
									<td class="text text-size-small text-black">{{username}}</td>
									<td class="text text-size-small text-black">{{showeddate}}</td>
								</tr>
								{{/each}} {{else}}
								<tr>
									<td class="text text-size-small text-black">None</td>
									<td class="text text-size-small text-black">None</td>
									<td class="text text-size-small text-black">None</td>
								</tr>
								{{/if}}
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</main>


<footer>
	<nav class="footer-navigation">
		<p target="blank" class="text text-size-small text-white">&copy; LenoxBot
		</p>
		<a id="twitter" target="blank" class="text text-size-small text-white" href="https://twitter.com/lenoxbot">
			<i class="fab fa-twitter" aria-hidden="true"></i>
		</a>
	</nav>
</footer>