{
  "name": "lenoxbot",
  "version": "1.12.11",
  "description": "",
  "main": "./src/index",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "./src/index",
    "test": "npx eslint .",
    "background": "screen -dmS lenoxbot ./src/index"
  },
  "author": "Gilles Heinesch",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "better-ms": "^1.0.5",
    "chalk": "^2.4.2",
    "columnify": "^1.5.4",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "discord.js": "github:discordjs/discord.js#master",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "ffmpeg-static": "^2.7.0",
    "got": "^9.6.0",
    "hbs": "^4.0.6",
    "heapdump": "^0.3.15",
    "http-errors": "^1.7.3",
    "i18n": "^0.8.4",
    "icecast-parser": "^3.2.1",
    "jsdom": "^15.2.0",
    "klasa": "github:dirigeants/klasa#settings",
    "klasa-dashboard-hooks": "github:dirigeants/klasa-dashboard-hooks#settings",
    "klasa-member-gateway": "github:dirigeants/klasa-member-gateway#settings",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "mongodb": "^3.3.3",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "node-opus": "^0.3.3",
    "parse-ms": "^2.1.0",
    "pornsearch": "^2.4.3",
    "youtube-crawl": "^0.0.1",
    "youtube-info": "^1.3.2",
    "youtube-playlist": "^1.0.2",
    "ytdl-core": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "brace-style": [
            "error",
            "stroustrup"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-unused-vars": [
            "warn"
        ],
        "no-var": [
            "off"
        ],
        "one-var": [
            "off"
        ],
        "no-multi-assign": [
            "off"
        ],
        "guard-for-in": [
            "off"
        ],
        "class-methods-use-this": [
            "off"
        ],
        "max-len": [
            "off"
        ]
    }
}
}
